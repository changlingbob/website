{
  "name": "website",
  "version": "0.0.1",
  "description": "Powerful Wizard Games website",
  "main": "www/index.html",
  "type": "module",
  "scripts": {
    "build": "vite build --emptyOutDir",
    "clear_jest": "jest --clearCache",
    "demo": "jest demo.ts --testMatch \\\"**/demo.ts\\\"",
    "demo:dev": "jest demo.ts --watch --testMatch \\\"**/demo.ts\\\"",
    "lint": "npm-run-all lint:prettier lint:eslint lint:stylelint",
    "lint:fix": "npm-run-all lint:prettier:fix lint:eslint:fix lint:stylelint:fix",
    "lint:eslint": "eslint \"**/*.tsx\" \"**/*.ts\"",
    "lint:eslint:fix": "eslint \"**/*.tsx\" \"**/*.ts\" --fix",
    "lint:prettier": "prettier --config .prettierrc.json --check **/*.tsx **/*.ts",
    "lint:prettier:fix": "prettier --config .prettierrc.json --write **/*.tsx **/*.ts",
    "lint:stylelint": "stylelint **/*.scss",
    "lint:stylelint:fix": "stylelint **/*.scss --fix",
    "start": "vite dev",
    "serve": "vite preview"
  },
  "author": "Greg Skinner",
  "license": "ISC",
  "dependencies": {
    "@staticcms/proxy-server": "4.0.4",
    "classnames": "2.5.1",
    "normalize.css": "8.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.22.3",
    "typescript": "5.4.5",
    "vike": "0.4.168",
    "vite": "5.2.8"
  },
  "devDependencies": {
    "@types/jest": "29.5.12",
    "@types/react": "18.2.78",
    "@types/react-dom": "18.2.25",
    "@typescript-eslint/eslint-plugin": "7.6.0",
    "@typescript-eslint/parser": "7.6.0",
    "@vitejs/plugin-react": "4.2.1",
    "eslint": "8.56.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "jest": "29.7.0",
    "npm-run-all": "4.1.5",
    "postcss-scss": "4.0.9",
    "prettier": "3.2.5",
    "react-test-renderer": "18.2.0",
    "sass": "1.75.0",
    "stylelint": "16.3.1",
    "stylelint-config-standard-scss": "13.1.0",
    "ts-jest": "29.1.2",
    "tslib": "2.6.2"
  }
}
